        <service id="diamante_automation.condition.changed_to" class="%diamante_automation.condition.changed_to.class%" parent="diamante_automation.condition.abstract_condition" shared="false">
        </service>

        <service id="diamante_automation.condition.like" class="%diamante_automation.condition.like.class%" parent="diamante_automation.condition.abstract_condition" shared="false">
        </service>

        <service id="diamante_automation.condition.created" class="%diamante_automation.condition.created.class%" parent="diamante_automation.condition.abstract_condition" shared="false">
        </service>

        <service id="diamante_automation.condition.eq" class="%diamante_automation.condition.eq.class%" parent="diamante_automation.condition.abstract_condition" shared="false">
        </service>

        <service id="diamante_automation.condition.false" class="%diamante_automation.condition.false.class%" parent="diamante_automation.condition.abstract_condition" shared="false">
