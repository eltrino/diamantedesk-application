        <service id="diamante_automation.condition.gte" class="%diamante_automation.condition.gte.class%" parent="diamante_automation.condition.abstract_condition" shared="false">
        </service>

        <service id="diamante_automation.condition.is_admin" class="%diamante_automation.condition.is_admin.class%" parent="diamante_automation.condition.abstract_condition" shared="false">
        </service>

        <service id="diamante_automation.condition.is_customer" class="%diamante_automation.condition.is_customer.class%" parent="diamante_automation.condition.abstract_condition" shared="false">
        </service>

        <service id="diamante_automation.condition.lt" class="%diamante_automation.condition.lt.class%" parent="diamante_automation.condition.abstract_condition" shared="false">
        </service>

        <service id="diamante_automation.condition.lte" class="%diamante_automation.condition.lte.class%" parent="diamante_automation.condition.abstract_condition" shared="false">
