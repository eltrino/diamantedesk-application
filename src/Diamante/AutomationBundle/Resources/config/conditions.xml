        <service id="diamante_automation.condition.neq" class="%diamante_automation.condition.neq.class%" parent="diamante_automation.condition.abstract_condition" shared="false">
        </service>

        <service id="diamante_automation.condition.not_like" class="%diamante_automation.condition.not_like.class%" parent="diamante_automation.condition.abstract_condition" shared="false">
        </service>

        <service id="diamante_automation.condition.removed" class="%diamante_automation.condition.removed.class%" parent="diamante_automation.condition.abstract_condition" shared="false">
        </service>

        <service id="diamante_automation.condition.true" class="%diamante_automation.condition.true.class%" parent="diamante_automation.condition.abstract_condition" shared="false">
        </service>

        <service id="diamante_automation.condition.updated" class="%diamante_automation.condition.updated.class%" parent="diamante_automation.condition.abstract_condition" shared="false">
        </service>

    </services>
</container>
